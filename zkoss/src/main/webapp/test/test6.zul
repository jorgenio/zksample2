				</listhead>
				<listitem label="item1" />
				<listitem label="item2" />
				<listitem label="item3" />
				<listitem label="item4" />
				<listitem label="item5" />
				<listitem label="item6" />
				<listitem label="item7" />
			</listbox>

			<paging id="pagingId" pageSize="5" totalSize="100"
				detailed="true" />
		</div>


	</window>
</zk>
