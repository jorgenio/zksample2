							</menupopup>
						</menu>
						<menuitem label="abbruch" />
					</menupopup>
				</menu>

				<menu label="Menu 2">
					<menupopup>
						<menuitem label="khkjhk" />
					</menupopup>

				</menu>

				<menu label="Menu 3"></menu>

				<menuitem label="Menu 4"></menuitem>

				<menuitem label="Menu 5"></menuitem>

				<menuitem label="Menu 6"></menuitem>


			</menubar>
		</div>

		<div>
			<listbox id="listBoxId" vflex="true" width="99.5%"
